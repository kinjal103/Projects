<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kinjal.foryou">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FeelingsActivity"
            android:label="@string/title_activity_feelings"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".HappyMoments"
            android:label="@string/title_activity_happy_moments"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SadMoments"
            android:label="@string/title_activity_sad_moments"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".LovelyMoments"
            android:label="@string/title_activity_lovely_moments"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MissingMe"
            android:label="@string/title_activity_missing_me"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
